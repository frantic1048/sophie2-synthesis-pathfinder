{
  "name": "sophie2-synthesis-pathfinder",
  "version": "1.0.0",
  "author": "frantic1048",
  "license": "MIT",
  "private": true,
  "scripts": {
    "fix": "yarn fix:rescript; yarn fix:prettier",
    "fix:rescript": "rescript format -all",
    "fix:prettier": "prettier --write .",
    "build": "yarn build:rescript; yarn build:webpack",
    "build:rescript": "rescript build -with-deps",
    "build:webpack": "webpack build --config webpack.config.prod.js",
    "dev:rescript": "rescript build -with-deps -w",
    "dev:webpack": "webpack serve --open --config webpack.config.dev.js"
  },
  "dependencies": {
    "@rescript/react": "0.10.3",
    "@ryyppy/rescript-promise": "2.1.0",
    "elkjs": "next",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-flow-renderer": "10.1.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "html-webpack-plugin": "5.5.0",
    "inject-body-webpack-plugin": "1.3.0",
    "prettier": "2.6.2",
    "rescript": "9.1.4",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1",
    "webpack-merge": "5.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
